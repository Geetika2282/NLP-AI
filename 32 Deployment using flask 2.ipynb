    "# URL Encoding\n",
    "from flask import Flask\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route('/')\n",
    "def hello_world():\n",
    "    return ('Welcome to AI')\n",
    "\n",
    "@app.route('/hello/<name>')\n",
    "def hello(name):\n",
    "    return ('Hello %s' %name)\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    app.run(debug=False)"
